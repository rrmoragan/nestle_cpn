    var typingShip; //Variable para contabilizar el tiempo de escritura del usuario
    function changeShippingPostcode(){
        clearTimeout(typingShip);//Termino de escribir
        var zipcode = $('shipping:postcode').getValue();
        if( (!Validation.get('IsEmpty').test(zipcode) &&  /(^\d{5}$)/.test(zipcode))){
            $('finding-cp-shipping').setStyle({ display:'inline-block' });
            var zipcodeURL = '<?php echo $this->getUrl('completeaddress/index/findAddress/') ?>zipcode/' + zipcode;
            //console.log(zipcodeURL);
            new Ajax.Request(zipcodeURL, {
                method: 'get',
                onSuccess: function(transport){
                    if(typeof transport.responseJSON!=='undefined' && transport.responseJSON.status === true && transport.responseJSON.results > 0){
                        var json = transport.responseJSON;
                        $('shipping:country_id').setValue('MX');
                        shippingRegionUpdater.update();
                        $('shipping:region_id').setValue(json.data[0].region_id);
                        $('shipping:city').setValue(json.data[0].municipality);
                        $('shipping:neighborhood_id').select('option').invoke('remove');
                        for(i=0;i<json.data.length;i++){
                            var opt = document.createElement('option');
                            opt.text = json.data[i].neighborhood;
                            opt.value = json.data[i].neighborhood;
                            $('shipping:neighborhood_id').options.add(opt);
                        }
                        $('shipping:neighborhood_id').setStyle({ display:'block' });
                        $('shipping:neighborhood').setStyle({ display:'none' });
                        $('finding-cp-shipping').setStyle({ display:'none' });
                        selectedShipNeighborhoodId();
                    }else{
                        $('shipping:neighborhood_id').setStyle({ display:'none' });
                        $('shipping:neighborhood').setStyle({ display:'block' });
                        $('finding-cp-shipping').setStyle({ display:'none' });
                    }
                },
                onException: function(){
                    $('finding-cp-shipping').setStyle({ display:'none' });
                }
            });
        }else{
            $('shipping:neighborhood_id').setStyle({ display:'none' });
            $('shipping:neighborhood').setStyle({ display:'block' });
            $('shipping:neighborhood').setValue('');
        }
    }
    
    function checkEndTypingShipping(){
        clearTimeout(typingShip);
        typingShip = setTimeout(changeShippingPostcode, 700);
    }
    
    function checkStartTypingShipping(){
        clearTimeout(changeShippingPostcode);
    }
    function selectedShipNeighborhoodId(){
        var neig = $('shipping:neighborhood_id').getValue();
        if(neig.length>0){
            $('shipping:neighborhood').setValue(neig);
        }else{
            $('shipping:neighborhood').setValue('');
        }
    }
    Event.observe('shipping:neighborhood_id', 'change', selectedShipNeighborhoodId);
    Event.observe('shipping:postcode', 'change', changeShippingPostcode);
    Event.observe('shipping:postcode', 'keyup', checkEndTypingShipping);
    Event.observe('shipping:postcode', 'keydown', checkStartTypingShipping);
    
    if($('shipping-address-select')){
